{
  "name": "agile-metrics-tools",
  "version": "1.0.0",
  "description": "",
  "author": "ferzerkerx",
  "license": "MIT",
  "scripts": {
    "start": "node dist/api/server.js",
    "start-local": "yarn build && yarn prepare-web && ES_HOST=http://localhost:9200 node dist/api/server.js",
    "prepare-web": "cp -R src/api/resources dist/",
    "build": "yarn compile",
    "compile": "tsc",
    "dev": "tsc & nodemon dist",
    "test": "jest --coverage",
    "lint": "eslint --fix 'src/**/*.ts'",
    "tsc": "tsc"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "bottleneck": "^2.19.5",
    "elasticsearch": "^16.7.3",
    "express": "4.19.2",
    "winston": "^3.13.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/jest": "27.0.3",
    "@types/node": "18.8.3",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.2",
    "jest": "26.6.3",
    "lint-staged": "13.0.3",
    "prettier": "2.5.1",
    "ts-jest": "26.5.6",
    "ts-loader": "8.0.10",
    "ts-node": "10.9.1",
    "typescript": "4.5.2"
  },
  "lint-staged": {
    "./**/*.{ts,js}": [
      "yarn lint"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
